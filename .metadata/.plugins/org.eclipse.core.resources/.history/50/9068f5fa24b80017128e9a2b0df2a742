
public class Programa {
	public static void main(String[] args) {
		
		Funcionario f1 = new Funcionario();
		f1.setDataDeEntrada(10/01/2012);
		f1.setDepartamento("Vendas");
		f1.setEstaNaEmpresa(true);
		f1.setNome("Eduardo");
		f1.setRg("45");
		f1.setSalario(1300.0);
		f1.calculaSalario();
		
	}

}
